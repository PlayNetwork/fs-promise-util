{
  "name": "fs-promise-util",
  "description": "a utility library for file system interaction on *nix machines",
  "private": false,
  "version": "1.0.0",
  "author": "",
  "engine": "node >= 4",
  "keywords": [
    "fs",
    "file system",
    "read",
    "write",
    "path"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:PlayNetwork/fs-promise-util.git"
  },
  "main": "./dist",
  "scripts": {
    "prepublish": "gulp build",
    "test": "gulp lint && gulp clean-reports && nyc -- _mocha ./test/unit",
    "test-unit": "gulp clean-reports && nyc -- _mocha ./test/unit"
  },
  "directories": {
    "test": "test"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "debug": "^2.2.0",
    "mkdirp": "0.5.1",
    "renege": "^0.1.4",
    "source-map-support": "^0.4.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "del": "^2.2.2",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-coveralls": "^0.1.4",
    "gulp-eslint": "^3.0.1",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^3.0.1",
    "gulp-sourcemaps": "^2.5.0",
    "gulp-util": "^3.0.7",
    "mock-spawn": "^0.2.6",
    "nyc": "^10.1.2",
    "sinon": "^1.17.7"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "src/json-rpc",
      "test/sandbox"
    ],
    "include": [
      "src"
    ],
    "instrument": false,
    "report-dir": "./reports",
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false
  }
}
